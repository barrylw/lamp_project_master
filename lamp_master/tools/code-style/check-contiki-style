#!/bin/sh
f=`basename $1`
mkdir /tmp/$$
cp $1 /tmp/$$/
cp $1 /tmp/$$/$f.orig
export INDENT_PROFILE=`dirname $0`/indent.pro
indent /tmp/$$/$f
diff /tmp/$$/$f.orig /tmp/$$/$f
rm -rf /tmp/$$
